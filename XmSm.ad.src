! XmSm app-defaults

*windowManager: PREFIX/bin/emwm
*launcher: PREFIX/bin/xmtoolbox
*enableLocking: True
*blankOnLock: True
*blankTimeout: 400
*numLockState: KEEP
*workspaceBackgroundColor: #4C719E
*lockBackgroundColor: #4C719E
*lockTimeout: 600
*unlockScreenTimeout: 16
*primaryXineramaScreen: 0
*showShutdown: True
*showReboot: True
